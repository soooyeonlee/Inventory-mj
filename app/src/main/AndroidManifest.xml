<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.recipe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoadingActivity"></activity>
        <activity android:name=".meatActivity" />
        <activity android:name=".fishActivity" />
        <activity android:name=".sidedishesActivity" />
        <activity android:name=".noodleActivity" />
        <activity android:name=".soupActivity" />
        <activity android:name=".foodlistActivity" />
        <activity android:name=".riceActivity" />
        <activity android:name=".Inform" />
        <activity android:name=".Menu" />
        <activity android:name=".menu" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>